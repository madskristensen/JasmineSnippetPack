<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>toHaveBeenCalled</Title>
    <Shortcut>thbc</Shortcut>
    <Author>Mads Kristensen</Author>
    <Description>Jasmine - returns true if the spy was called</Description>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>arg</ID>
        <Default>argument</Default>
      </Literal>
    </Declarations>
    <Code Language="TypeScript">
      <![CDATA[expect($arg$).toHaveBeenCalled();$end$]]>
    </Code>
  </Snippet>
</CodeSnippet>