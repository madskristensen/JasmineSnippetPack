<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>callsFirst</Title>
    <Shortcut>cf</Shortcut>
    <Author>Mads Kristensen</Author>
    <Description>Jasmine - returns the context (the this) and arguments for the first call</Description>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>spy</ID>
        <ToolTip></ToolTip>
        <Default>spy</Default>
      </Literal>
    </Declarations>
    <Code Language="JavaScript">
      <![CDATA[$spy$.calls.first()$end$]]>
    </Code>
  </Snippet>
</CodeSnippet>