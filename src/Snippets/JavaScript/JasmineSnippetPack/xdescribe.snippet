<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>xDescribe</Title>
    <Shortcut>xdesc</Shortcut>
    <Author>Mads Kristensen</Author>
    <Description>Jasmine - these suites and any specs inside them are skipped when run and thus their results will not appear in the results</Description>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>name</ID>
        <ToolTip></ToolTip>
        <Default>name of the group</Default>
      </Literal>
    </Declarations>
    <Code Language="JavaScript">
      <![CDATA[xdescribe('$name$', function() {
  $end$
});]]>
    </Code>
  </Snippet>
</CodeSnippet>