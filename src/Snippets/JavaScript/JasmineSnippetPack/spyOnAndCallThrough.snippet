<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>spyOnAndCallThrough</Title>
    <Shortcut>sct</Shortcut>
    <Author>Mads Kristensen</Author>
    <Description>Jasmine - tracks all calls to it and delegates to the actual implementation</Description>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>object</ID>
        <Default>object</Default>
      </Literal>
      <Literal>
        <ID>method</ID>
        <Default>method</Default>
      </Literal>
    </Declarations>
    <Code Language="JavaScript">
      <![CDATA[spyOn($object$, $method$).and.callThrough();$end$]]>
    </Code>
  </Snippet>
</CodeSnippet>