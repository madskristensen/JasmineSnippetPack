<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>toThrow</Title>
    <Shortcut>tt</Shortcut>
    <Author>Mads Kristensen</Author>
    <Description>Jasmine - expects that the specified method will throw</Description>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>arg</ID>
        <Default>argument</Default>
      </Literal>
    </Declarations>
    <Code Language="JavaScript">
      <![CDATA[expect($arg$).toThrow();$end$]]>
    </Code>
  </Snippet>
</CodeSnippet>