<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>toMatch</Title>
    <Shortcut>tm</Shortcut>
    <Author>Mads Kristensen</Author>
    <Description>Jasmine - Expects the first argument to match the second one</Description>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>expected</ID>
        <Default>expected</Default>
      </Literal>
      <Literal>
        <ID>value</ID>
        <Default>value</Default>
      </Literal>
    </Declarations>
    <Code Language="JavaScript">
      <![CDATA[expect($expected$).toMatch($value$);$end$]]>
    </Code>
  </Snippet>
</CodeSnippet>